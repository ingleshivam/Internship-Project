    <!-- Preloader Start -->
    @{
        Int64 InvID = Convert.ToInt64(Context.Session.GetString("InvestorID"));
        Int64 UserID = Convert.ToInt64(Context.Session.GetString("UserID"));
    }
    <div id="preloader-active">
        <div class="preloader d-flex align-items-center justify-content-center">
            <div class="preloader-inner position-relative">
                <div class="preloader-circle"></div>
                <div class="preloader-img pere-text">
                    <img src="~/frontend/assets/img/logo/IdeaFundu.png" alt="">
                </div>
            </div>
        </div>
    </div>
    <!-- Preloader Start -->
    
    <header style="width: 100%;background-image: url('/frontend/assets/img/landingpageimages/landingpageimg5blur5.jpg');background-repeat: no-repeat;background-size: cover;background-position: center;">
        <!-- Header Start -->
        @if(InvID != 0 || UserID  != 0){
            <div class="alert alert-success text-center rounded-0" style="font-weight:bold;"><span>To access the Users/Investors dashboard, Click on "Dashboard"</span></div>
        }else{
            <div class="alert alert-success text-center rounded-0" style="font-weight:bold;">To access the Admin dashboard, @Html.ActionLink("Click Here !","Index","AdminHome",new {area = "AdminArea"},new {@class="text-primary"})</div>
        }
       
       <div class="header-area header-transparrent">
           <div class="headder-top header-sticky">
                <div class="container">
                    <div class="row align-items-center">
                        <div class="col-lg-3 col-md-2">
                            <!-- Logo -->
                            <div class="logo">
                                <h2 style="color:white; font-weight:bolder;">IdeaFundu</h2>
                                @*<a href="index.html"><img src="~/frontend/assets/img/logo/IdeaFundu.png"alt="" width="190"></a>*@
                            </div>  
                        </div>
                        <div class="col-lg-9 col-md-9">
                            <div class="menu-wrapper">
                                <!-- Main-menu -->
                                <div class="main-menu">
                                    <nav class="d-none d-lg-block" >
                                        <ul id="navigation">
                                            <li><a asp-action="Index"  style="color:white; font-weight:bold;">Home</a></li>
                                            <li><a href="job_listing.html" style="color:white; font-weight:bold;" >Find a Jobs </a></li>
                                            <li><a asp-action="Dashboard"  style="color:white; font-weight:bold;">Dashboard</a></li>
                                            <li><a href="#"  style="color:white; font-weight:bold;">Page</a>
                                                <ul class="submenu">
                                                    <li><a href="blog.html">Blog</a></li>
                                                    <li><a href="single-blog.html">Blog Details</a></li>
                                                    <li><a href="elements.html">Elements</a></li>   
                                                    <li><a href="job_details.html">job Details</a></li>
                                                </ul>
                                            </li>
                                            <li><a href="contact.html"  style="color:white; font-weight:bold;">Contact</a></li>
                                        </ul>
                                    </nav>
                                </div>          
                                <!-- Header-btn -->

                                @if(InvID == 0 && UserID  == 0){
                                    <div class="header-btn f-right d-flex">
                                        <div class="dropdown show">
                                            <a class="btn head-btn1 dropdown-toggle" style="border:3px solid white; color:white;font-weight:bold;background-color:transparent;" href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                Register
                                            </a>
                                            <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                                                <a class="dropdown-item" asp-action="SignUp" asp-controller="ManageUsers">User</a>
                                                <a class="dropdown-item" asp-action="SignUp" asp-controller="ManageInvestors">Investor</a>
                                            </div>
                                        </div>
                                        <div class="dropdown show ml-1">
                                            <a class="btn head-btn2 dropdown-toggle" style="border:3px solid white; color:white;font-weight:bold;"href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                Login
                                            </a>
                                            <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                                                <a class="dropdown-item" asp-action="SignIn" asp-controller="ManageUsers">User</a>
                                                <a class="dropdown-item" asp-action="SignIn" asp-controller="ManageInvestors">Investor</a>
                                            </div>
                                        </div>
                                    </div>
                                }else{
                                    <div class="header-btn f-right d-flex">
                                        <div class="dropdown show ml-1">
                                            <a style="border:3px solid white; color:white;font-weight:bold;" class="btn head-btn2 dropdown-toggle" href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                @Context.Session.GetString("UserName")
                                                @Context.Session.GetString("InvestorName")
                                            </a>
                                            <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                                                <a class="dropdown-item" asp-action="SignOut" asp-controller="Home" asp-area="">SignOut </a>                                               
                                            </div>
                                        </div>
                                    </div>
                                }
                            </div>
                        </div>
                        <!-- Mobile Menu -->
                        <div class="col-12">
                            <div class="mobile_menu d-block d-lg-none"></div>
                        </div>
                    </div>
                </div>
           </div>
       </div>
        <!-- Header End -->
        @RenderSection("DropDown",false)
    </header>

